    Title="MainWindow" Visibility="Visible" Width="441" Height="385" BorderBrush="Black" Background="{x:Null}" OpacityMask="Black">
    <WindowChrome.WindowChrome>
        <WindowChrome CaptionHeight="30"
                      ResizeBorderThickness="4" />
    </WindowChrome.WindowChrome>
    <Grid>
        <WebBrowser HorizontalAlignment="Left" Height="332" Margin="0,22,0,0" VerticalAlignment="Top" Width="433"/>
